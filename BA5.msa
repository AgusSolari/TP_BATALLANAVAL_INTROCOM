* Esta subrutina hace pasar por display un cartel felicitando al ganador		*
*****************************************************************************************
windsp	equ	*
	psha
	pshb
	pshy
	pshx

	jsr 	clear
	
	ldy	#RAM
	ldaa	#P1
	cmpa	0,y
	bne	P1WINNT				;verifico que jugador es el que ganó

	ldx	#P1WIN				;cargo el registro X con la posicion del comienzo del string
	bra	prntwin

P1WINNT	ldx	#P2WIN				;cargo el registro X con la posicion del comienzo del string

prntwin	equ	*

	ldy	#10				;creo un contador de 10 unidades en el registro Y. La finalidad de este contador es utlizarlo para conocer cuando el programa dejara de imprimir caracteres en el display
	
	
	clr	0,x				;se limpia la posicion la cual esta posicionada el registro X. Este cero estara ubicado a para indicar el comienzo o final de la palabra

	dex					;se drcrementa el registro X para que nuestro puntero quede posicionado 3 lugares por detras
	clr	0,x				;de la primera letra a mostrar en el diplay

	dex
	clr	0,x				;a su vez cada vez que se decrementa el valor del reg. X se va limpiando las respectivas posiciones, para evitar que se impriman caracteres que no fomran parte de la palabra

	dex
	clr	0,x

	jsr	clear				;salto a la subrutinia clear la cual se encarga de limpiar los caracteres que se encuentren en el display


REP3	ldaa	0,x				;cargo en el registro A el caracter que inidca el puntero X
	ldab	#3				;cargo en la posicion 3 del display el caracter obtenido del registro A
	jsr	outchar				;imprimo el caracter en el display

	ldaa	1,x				;cargo en el registro A el caracter siguinte al que fue impreso
	ldab	#2				;cargo en la posicion 2 del display el caracter consecutivo al impreso
	jsr	outchar				;imprimo el caracter en el display


	ldaa	2,x				;cargo el siguiente caracter en el registro A
	ldab	#1				;cargo en la posicion 1 del display el caracter que fue obtenido
	jsr	outchar				;imprimo el caracter en el display


	ldaa	3,x				;cargo en el registro A el ultimo caracter a imprimir antes incrementar el registro X e imprimir en el display nuevos caracteres

	bne	REP4				;analizo si es cero el ultimo caracter a imprimir. La finalidad de este dato es comprobar si  el puntero X esta llegando a su fin e ir decrementando el contador para que no imprima caracteres de mas)
	dey					;decremento el contador IY, ya que el caracter cargado en el registro A fue un cero
	beq	REP5				;en caso de que el contador sea cero quiere decir que el display ya imprimo todos los caracteres de la palabra en el display y este llega a su fin

REP4	ldab	#0				;en caso de que el caracter no haya sido cero o el contador no finalizo, se imprime el caracter del registro A en la posicion cero del display

	jsr	outchar				;se imprimer el caracter en el display
	jsr	DELAY				;se hace un delay 
	inx					;se incrementa el registro X para que el programa imprima en el display nuevos caracteres
	bra	REP3				;se hace un salto para cargar en el display nuevos caracteres
	
REP5	equ	*

	pulx
	puly
	pulb
	pula

	rts
