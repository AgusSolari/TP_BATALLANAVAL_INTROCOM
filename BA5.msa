	anda	#AGUA			;Filtro el valor de la casilla para que verifique el tipo de embarcacion
	cmpa	#SUBMARI
	bne	danont
	
	jsr	SubHun			;En caso de que en la fila y columna disparada se encuentre un submarino se colocara en el bitmap el color rojo y se cargara su respectivo estado en la matriz

	ldaa	#ROJO
	bra	pintar

danont	ldaa	#DANADO			;si no esta dañado y tampoco es un submarino o agua, entonces es un carguero (y por ende habria que dañarlo y poner el color amarillo)
	ldab	#AMARIL
	aba
	staa	0,x			;cargo en la matriz del jugador el estado del barco

* Ahora almaceno el estado en una matriz de las embarcaciones de cada jugador
	jsr	PlObt2			;esta subrutina me devuelve en IX un puntero posicionado en el comienzo de la matriz de las embaraciones de cada jugador
	jsr	EmbObt			;esta subrutina me posiciona el puntero IX la posicion de la celda disparada

	ldaa	#2			;contador de dos para analizar en donde poner el esatdo de los cargueros

